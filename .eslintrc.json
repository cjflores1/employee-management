{
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "no-useless-catch": "off"
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}